{
    "compilerOptions": {
      "target": "ES2022",
      "module": "ESNext",
      "lib": ["DOM", "ES2022"],
      "jsx": "react-jsx",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "isolatedModules": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"]
}
  